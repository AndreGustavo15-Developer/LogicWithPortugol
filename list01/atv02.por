programa {
  funcao inicio()
  {
    inteiro valor1
    inteiro valor2

    escreva("Digite um número: ")
    leia(valor1)
    escreva("Digite outro numero: ")
    leia(valor2)

    escreva("A subtração é: ", valor1 - valor2, "\n")
    escreva("A multiplicação é: ", valor1 * valor2, "\n")
    escreva("A divisão é: ", valor1 / valor2, "\n")

  }
}
